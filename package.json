{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:enhanced": "vite --config vite.config.enhanced.ts",
    "dev:optimized": "vite --config vite.config.optimized.ts",
    "build": "vite build",
    "build:typecheck": "tsc -b && vite build",
    "build:enhanced": "tsc -b && vite build --config vite.config.enhanced.ts",
    "build:optimized": "tsc -b && vite build --config vite.config.optimized.ts",
    "build:analyze": "npm run build:optimized && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:optimized": "vite preview --config vite.config.optimized.ts",
    "fix-project-db": "node fix-project-db-direct.js",
    "perf:audit": "lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html",
    "perf:measure": "node -e \"console.log('Performance measurement tools ready')\"",
    "optimize:images": "echo 'Consider using imagemin or sharp for image optimization'",
    "test:performance": "echo 'Run lighthouse audit after starting preview server'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "seed:projects": "node scripts/seed-test-projects.js",
    "test:e2e:report": "playwright show-report"
  },
