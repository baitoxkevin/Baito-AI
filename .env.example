# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key (for admin operations - NEVER expose in client!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Supabase Management API Token (for MCP tools, migrations, logs)
SUPABASE_ACCESS_TOKEN=your-access-token-here

# OpenRouter API Key (for AI chatbot)
# Get your key from: https://openrouter.ai/keys
VITE_OPENROUTER_API_KEY=your-openrouter-api-key-here

# Groq API Key (for voice transcription AND chat LLM)
# Get your key from: https://console.groq.com/keys
#
# Used for:
# 1. Voice Transcription (Whisper Large v3 Turbo)
#    - $0.04/hour (multilingual, supports Malaysian rojak)
# 2. Chat LLM (Qwen3 32B)
#    - $0.29/M input tokens, $0.59/M output tokens
#    - 119+ languages including Malay, Chinese dialects
#    - Best for Malaysian rojak code-switching
#
# NOTE: Add this to Supabase Edge Functions secrets too:
# supabase secrets set GROQ_API_KEY=your-key-here
GROQ_API_KEY=your-groq-api-key-here

# Cache Configuration (Optional - uses in-memory cache if not set)
# REDIS_URL=redis://localhost:6379
# UPSTASH_REDIS_URL=your-upstash-url
# UPSTASH_REDIS_TOKEN=your-upstash-token

# Database Optimization
DB_POOL_MIN=2
DB_POOL_MAX=10
DB_STATEMENT_TIMEOUT=30000
DB_CONNECTION_TIMEOUT=5000

# Performance Monitoring
ENABLE_CACHE_STATS=true
CACHE_TTL_PROJECT=300
CACHE_TTL_USER=900
CACHE_TTL_ANALYTICS=3600

# Test Environment Configuration
TEST_ENV=local
BASE_URL=http://localhost:5173
API_URL=http://localhost:3000/api

# Staging URLs
STAGING_URL=https://staging.baito.events
STAGING_API_URL=https://api-staging.baito.events

# Production URLs
PROD_URL=https://baito.events
PROD_API_URL=https://api.baito.events

# Test User Credentials
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=TestPassword123!
